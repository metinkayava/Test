{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "base": {
      "type": "string"
    },
    "rates": {
      "type": "object",
      "properties": {
        "USD": {
          "type": "number"
        },
        "GBP": {
          "type": "number"
        }
      },
      "required": [
        "USD",
        "GBP"
      ]
    },
    "date": {
      "type": "string"
    }
  },
  "required": [
    "base",
    "rates",
    "date"
  ]
}